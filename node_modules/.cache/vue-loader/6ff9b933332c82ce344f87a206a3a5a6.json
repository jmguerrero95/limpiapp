os.post(path, this.form).then((response)=> {\n                this.form.firs_name\n                this.form.last_name\n                this.form.user_email\n                this.form.password\n                this.form.tipo\n                this.popup({tipo:3,overlay:true})\n                  setTimeout(alertFunc, 2000)\n                  function alertFunc(){\n                    VueRouter.push('login')\n                  }\n              })\n              .catch((error) => {\n                this.popup({tipo:4,overlay:true})\n              })\n        }\n      }\n    }\n  },\n    ...mapMutations([\"popup\"]),\n}\n}\n</script>\n<style scope>\n.v-btn {\n  text-transform: none;\n  font-weight: 300;\n}\n</style>"]}]}